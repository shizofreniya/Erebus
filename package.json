	"bugs": {
		"url": "https://github.com/shizofreniya/Erebus/issues"
	},
	"homepage": "https://github.com/shizofreniya/Erebus#readme",
	"devDependencies": {
		"@types/node": "^20.1.0",
		"@types/ws": "^8.5.4",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"ws": "^8.13.0"
	}
}
